{
  "name": "backing-tracks-isomorphic",
  "version": "1.0.4",
  "description": "Shared Backing Tracks code that runs on client and server.",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "node --watch-path=./src --watch-preserve-output onFileChange.mjs",
    "build": "tsup"
  },
  "engines": {
    "npm": ">=10.2.3 <11.0.0",
    "node": ">=20.19.0 <21.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.mjs",
      "require": "./dist/schemas/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "validator": "^13.15.0 <14.0.0",
    "zod": ">=3.24.2 <4.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    },
    "validator": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/validator": "^13.15.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "validator": "^13.15.0",
    "zod": "^3.24.3"
  }
}
